order += "wireless wlp2s0"
order += "disk /"
order += "battery 0"
order += "tztime local"

wireless wlp2s0 {
  format_up = "WLAN: %essid(%quality)"
  format_down = "WLAN: down"
}
battery 0 {
  format = "%status %percentage %remaining"
}

tztime local {
  format = "%d-%m-%Y %H:%M:%S"
}

disk "/" {
  format = "Free space: %free"
}
